{
  "arrowParens": "avoid",
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "^@/(.*)$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,

  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-sort-imports"
  ]
}
